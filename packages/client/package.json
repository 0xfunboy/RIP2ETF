{
  "name": "@elizaos/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --watch"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "5.9.x"
  },
  "dependencies": {
    "@elizaos/api-client": "workspace:*"
  }
}